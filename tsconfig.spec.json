{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": false,
    "target": "es6",
    "lib": ["es6"],
    "noImplicitAny": false,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "frameworks": ["jasmine"],
  "path": {
    "*" : [
      "node_modules/aws-sdk/*"
    ]
  },
  "filesGlob":[
    "src/**/*.ts",
    "node_modules/typescript/lib/lib.es6.d.ts"
  ],
  "exclude":[
    "dist",
    "node_modules"
  ]
}
